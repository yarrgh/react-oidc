{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "oidc-client": "^1.11.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@yarrgh/react-oidc",
  "author": "Trevor Hackett",
  "module": "dist/react-oidc.esm.js",
  "size-limit": [
    {
      "path": "dist/react-oidc.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-oidc.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "husky": "^7.0.0",
    "react": "^17.0.2",
    "oidc-client": "^1.11.5",
    "react-dom": "^17.0.2",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
